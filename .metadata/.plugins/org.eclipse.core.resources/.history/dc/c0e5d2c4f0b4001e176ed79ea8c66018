package jp.gr.java_conf.shoji.util.message;

public interface SampleMessageBundle {
    public static final SampleMessageBundle MESSAGES = MessageUtils
            .createMessageFormatterFormatMessageBundle(SampleMessageBundle.class);

    @Messages( { @Message("default message"),
            @Message(value = "デフォルトのメッセージ", locale = "ja") })
    String message1();

    @Messages( { @Message("default message %1$s, %2$s") })
    String message2(String a, String b);
}
